{
    "name": "vortexgin/amp-library",
    "description": "A set of useful classes and utilities to convert html to AMP html (See https://www.ampproject.org/). Fork version from lullabot/amp",
    "authors": [
        {
            "name": "Gin Vortex",
            "email": "vortexgin@gmail.com"
        }
    ],
    "license": "Apache-2.0",
    "keywords": ["Symfony", "AMP", "mobile"],
    "homepage": "https://github.com/vortexgin/amp-library",
    "require" : {
        "php" : ">=5.5.0",
        "querypath/QueryPath": ">=3.0.4",
        "sebastian/diff": "~1.2",
        "symfony/console": "^2.7.0",
        "marc1706/fast-image-size": "1.*",
        "masterminds/html5": "^2.2.0",
        "sabberworm/php-css-parser": "^8.0.0",
        "guzzlehttp/guzzle": "~5.0"
    },
    "autoload": {
        "classmap": ["src/Spec/validator-generated.php"],
        "psr-4": {"Lullabot\\AMP\\": "src"}
    },
    "require-dev": {
        "php" : ">=5.5.0",
        "phpunit/phpunit": "4.8.*"
    }
}